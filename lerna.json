{
  "npmClientArgs": [
    "--no-package-lock"
  ],
  "command": {
    "add": {
      "exact": true,
      "noBootstrap": true
    },
    "run": {
      "stream": true
    },
    "version": {
      "yes": true
    },
    "bootstrap": {
      "ignore": []
    },
    "publish": {
      "ignore": [],
      "ignoreChanges": [
        "**/.bin/**"
      ],
      "registry": "https://registry.npmjs.org/"
    }
  },
  "npmClient": "yarn",
  "useWorkspaces": true,
  "version": "1.0.1"
}